{
  "name": "discover-my-newsletters",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "fix-types": "node scripts/fix-types.mjs",
    "fix:types": "node --loader ts-node/esm scripts/comprehensive-type-fixer.ts",
    "type-fix": "tsx scripts/comprehensive-type-fixer.ts",
    "deploy:staging": "firebase use staging && npm run build && firebase deploy --only hosting:staging",
    "deploy:production": "firebase use production && npm run build && firebase deploy --only hosting:production"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/forms": "^0.5.10",
    "firebase": "^10.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.1",
    "react-router-dom": "^6.21.1",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@babel/generator": "^7.26.3",
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "@babel/types": "^7.26.3",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/testing-library__react": "^10.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lucide-react": "^0.469.0",
    "msw": "^2.7.0",
    "plausible-tracker": "^0.3.9",
    "prettier": "^3.4.2",
    "recharts": "^2.15.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "uuid": "^11.0.4",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vitest-mock-extended": "^1.3.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
